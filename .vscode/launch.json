  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "rink eval json",
      "cargo": {
        "args": [
          "build",
          "--bin=eval",
          "--package=rink"
        ],
        "filter": {
          "name": "eval",
          "kind": "bin"
        }
      },
      "args": ["--json", "${file}"],
      "cwd": "${workspaceFolder}"
    }
  ]
}