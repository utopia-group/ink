
program

typedecl row struct {n0:int; n1:int; n2:int; n3:int;};

row *input;
row x;
row output = {n0=0, n1=0, n2=0, n3=0};

int temp2;
int temp3;

for (x in input){
  if(output.n2 > x.n3) {
    temp2 = output.n2;
  } else {
    temp2 = x.n3;
  }
  if(output.n3 > x.n3) {
    temp3 = output.n3;
  } else {
    temp3 = x.n3;
  }
  output = {n0=x.n0, n1=output.n1+1, n2=temp2, n3=temp3};
}
return output;

