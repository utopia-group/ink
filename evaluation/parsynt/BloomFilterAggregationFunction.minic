
program

typedecl row struct {n0:int; n1:int;};
typedecl res struct {n0:int; n1:int *;};

row *input;
row x;
res output = {n0=0, n1=[]};

int *tmp = [];
int b;

for (x in input){
  tmp = output.n1;
  tmp += [x.n1];
  b = 0;
  if (x.n0 > output.n0){
    b = x.n0;
  } {
    b = output.n0;
  }
  output = {n0=b, n1=tmp};
}
return output;

