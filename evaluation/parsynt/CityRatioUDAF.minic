program

typedecl row struct {n0:int; n1:int;};

row * output = [];
row * tmp;
row y;

int * input;
int x;

boolean to_add;

for(x in input) {
  tmp = [];
  to_add = true;
  for(y in output){
      if(y.n0 == x) {
          tmp += [{n0=y.n0, n1=y.n1+1}];
          to_add = false;
      } else {
          tmp += [{n0=y.n0, n1=y.n1}];
      }
  }
  if(to_add){
    tmp += [{n0=x, n1=1}];
  }
  output = tmp;
}
return output as set;
