
program

typedecl row struct {n0:int; n1:int; n2:int;};
typedecl res struct {n0:int; n1:int *; n2:int; n3:int;};

row *input;
row x;
res output = {n0=0, n1=[], n2=0, n3=0};

int tmp0;
int * tmp1 = [];
int tmp2;
int tmp3;

for (x in input){
  tmp0 = 0;
  tmp1 = output.n1;
  tmp2 = 0;
  tmp3 = 0;

  
  if (x.n0 == 0){
    tmp0 = output.n0+1;
    tmp1 += [x.n0];
  } else {
    tmp0 = output.n0;
  }

  if (output.n2 == 0){
    tmp2 = x.n2;
  } else {
    tmp2 = output.n2;
  }

  if (x.n1 == 1){
    tmp3 = output.n0;
  } else {
    tmp3 = output.n3;
  }
  output = {n0=tmp0, n1=tmp1, n2=tmp2, n3=tmp3};
}
return output;

