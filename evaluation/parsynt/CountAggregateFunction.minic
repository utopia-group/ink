
program

typedecl row struct {n0:int; n1:int;};
typedecl res struct {n0:int; n1:int; n2:int;};

row *input;
row x;
res output = {n0=0, n1=0, n2=0};

int temp0;
int temp1;

for (x in input){
  if(output.n0 > x.n0) {
    temp0 = output.n0;
  } else {
    temp0 = x.n0;
  }
  if(output.n1 > x.n1) {
    temp1 = output.n1;
  } else {
    temp1 = x.n1;
  }
  output = {n0=temp0, n1=temp1, n2=output.n2+1};
}
return output;

